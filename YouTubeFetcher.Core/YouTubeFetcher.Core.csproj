<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <AssemblyName>YouTubeFetcher.Core</AssemblyName>
    <RootNamespace>YouTubeFetcher.Core</RootNamespace>
    <Version>1.0.0</Version>
    <Description>YouTubeFetcher.Core is a .NET Standard class library which can be used to get streams from YouTube.</Description>
    <PackageProjectUrl>https://github.com/arisjavet/YouTubeFetcher</PackageProjectUrl>
    <RepositoryUrl>https://github.com/arisjavet/YouTubeFetcher</RepositoryUrl>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <Copyright>© Aris Javet. All rights reserved.</Copyright>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <Authors>Aris Javet</Authors>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Options" Version="3.1.5" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>
  
</Project>
